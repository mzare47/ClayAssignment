@model IEnumerable<LockDto>
@{
    ViewData["Title"] = "Home Page";
}
<input id="uId" type="hidden" value="@ViewBag.userId"/>
<input id="token" type="hidden" value="@ViewBag.token"/>
<div class="row justify-content-center">
    @foreach(var lck in Model){
        <div class="col-sm-6 col-md-4 col-lg-3 text-center pb-2">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">@lck.Name</h5>
                    <p class="card-text">@lck.Location</p>
                    <label class="switch">
                      <input class="lock-checkbox" type="checkbox" id="@lck.LockId" @(lck.IsLocked ? "" : "checked")>
                      <span class="slider"></span>
                    </label>
                </div>
            </div>
      </div>
    }
</div>

